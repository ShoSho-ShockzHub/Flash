local DiscordLib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/discord%20lib.txt")()

local win = DiscordLib:Window("The Flash Universe")

local serv = win:Server("Main", "http://www.roblox.com/asset/?id=6031075938")

local MainSector = serv:Channel("Main Sector")

if getgenv().AutoAntiKick == true then
local OldNameCall = nil
OldNameCall = hookmetamethod(game, "__namecall", function(...)
    local Args = {...}
    local Self = Args[1]
if getnamecallmethod() == "Kick" and Self == game.Players.LocalPlayer then
return print("game tried to kick lel!")
end
    return OldNameCall(...)
end)
end

function AntiKick()
local OldNameCall = nil
OldNameCall = hookmetamethod(game, "__namecall", function(...)
    local Args = {...}
    local Self = Args[1]
if getnamecallmethod()=="Kick" and Self==game.Players.LocalPlayer then
return print("game tried to kick lel!")
end
    return OldNameCall(...)
end)
end

MainSector:Button("Anti Kick / Ban", function()
AntiKick()
end)

local sldr = MainSector:Slider("WalkSpeed", 16, 1200, 0, function(v)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
end)

local sldr = MainSector:Slider("JumpPower", 50, 1200, 0, function(v)
game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
end)

local Player = {}
for i,v in pairs(game.Players:GetPlayers()) do
    table.insert(Player, v.Name)
end

local teleport = MainSector:Dropdown("Teleport To Player", Player, function(Player)
game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Players")[Player].Character.HumanoidRootPart.CFrame 
end)

MainSector:Seperator()
